</span><span><span class="hljs-comment"># Author: Flyxion</span></span><span>
</span><span><span class="hljs-comment"># License: AGPLv3</span></span><span>

pragma version </span><span><span class="hljs-string">"0.2"</span></span><span>
pragma choice off          </span><span><span class="hljs-comment"># disable nonconstructive choice by default</span></span><span>

</span><span><span class="hljs-keyword">from</span></span><span> spherepop_rsvp </span><span><span class="hljs-keyword">import</span></span><span> entropy_cost, RSVPState

</span><span><span class="hljs-comment"># -------------------------------------------------------------</span></span><span>
</span><span><span class="hljs-comment"># Merge calculus primitives</span></span><span>
</span><span><span class="hljs-comment"># -------------------------------------------------------------</span></span><span>

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">MergeConflict</span></span><span>(</span><span><span class="hljs-title class_ inherited__">Exception</span></span><span>): </span><span><span class="hljs-keyword">pass</span></span><span>
</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">InstantiationError</span></span><span>(</span><span><span class="hljs-title class_ inherited__">Exception</span></span><span>): </span><span><span class="hljs-keyword">pass</span></span><span>

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">merge</span></span><span>(</span><span><span class="hljs-params">a, b</span></span><span>):
    <span class="hljs-string">"""
    Entropy-aware merge ().
    Commutative, associative, idempotent.
    Falls back to nonconstructive select if #pragma choice is on.
    """</span>
    </span><span><span class="hljs-keyword">if</span></span><span> a == b:
        </span><span><span class="hljs-keyword">return</span></span><span> a
    cost = entropy_cost(a, b)
    </span><span><span class="hljs-keyword">if</span></span><span> cost &lt; </span><span><span class="hljs-built_in">float</span></span><span>(</span><span><span class="hljs-string">"inf"</span></span><span>):
        </span><span><span class="hljs-keyword">return</span></span><span> combine(a, b, cost)
    </span><span><span class="hljs-keyword">if</span></span><span> pragma_enabled(</span><span><span class="hljs-string">"choice"</span></span><span>):
        </span><span><span class="hljs-keyword">return</span></span><span> select([a, b])        </span><span><span class="hljs-comment"># Axiom of Choice </span></span><span>
    </span><span><span class="hljs-keyword">raise</span></span><span> MergeConflict(</span><span><span class="hljs-string">f"Non-coherent merge of <span class="hljs-subst">{a}</span></span></span><span>, </span><span><span class="hljs-subst">{b}</span></span><span>")

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">combine</span></span><span>(</span><span><span class="hljs-params">a, b, cost=<span class="hljs-literal">None</span></span></span><span>):
    </span><span><span class="hljs-string">"""Negentropic fusion of coherent terms."""</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(a, (</span><span><span class="hljs-built_in">int</span></span><span>, </span><span><span class="hljs-built_in">float</span></span><span>)) </span><span><span class="hljs-keyword">and</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(b, (</span><span><span class="hljs-built_in">int</span></span><span>, </span><span><span class="hljs-built_in">float</span></span><span>)):
        w = </span><span><span class="hljs-number">1.0</span></span><span> / (</span><span><span class="hljs-number">1.0</span></span><span> + cost) </span><span><span class="hljs-keyword">if</span></span><span> cost </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0.5</span></span><span>
        </span><span><span class="hljs-keyword">return</span></span><span> (</span><span><span class="hljs-number">1</span></span><span> - w) * a + w * b
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(a, </span><span><span class="hljs-built_in">str</span></span><span>) </span><span><span class="hljs-keyword">and</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(b, </span><span><span class="hljs-built_in">str</span></span><span>):
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">f"<span class="hljs-subst">{a}</span></span></span><span></span><span><span class="hljs-subst">{b}</span></span><span>"
    </span><span><span class="hljs-keyword">return</span></span><span> (a, b)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">normalize</span></span><span>(</span><span><span class="hljs-params">a, mod=<span class="hljs-literal">None</span></span></span><span>):
    <span class="hljs-string">"""
    Canonical representative of a quotient.
    With integer mod -&gt; arithmetic normalization.
    Otherwise calls user-defined canonical form.
    """</span>
    </span><span><span class="hljs-keyword">if</span></span><span> mod </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> canonical(a)
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(a, </span><span><span class="hljs-built_in">int</span></span><span>) </span><span><span class="hljs-keyword">and</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(mod, </span><span><span class="hljs-built_in">int</span></span><span>):
        </span><span><span class="hljs-keyword">return</span></span><span> a % mod
    </span><span><span class="hljs-keyword">return</span></span><span> canonical_mod(a, mod)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">instantiate</span></span><span>(</span><span><span class="hljs-params">term, =<span class="hljs-literal">None</span></span></span><span>, v=</span><span><span class="hljs-literal">None</span></span><span>, S=</span><span><span class="hljs-literal">None</span></span><span>):
    <span class="hljs-string">"""
    RSVP instantiation: evaluate a term within a field plenum.
    Enforces finite coherence (entropy cost &lt; ).
    """</span>
