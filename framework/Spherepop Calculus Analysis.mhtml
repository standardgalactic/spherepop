</span><span><span class="hljs-built_in">&amp;</span></span><span> </span><span><span class="hljs-built_in">&amp;</span></span><span> </span><span><span class="hljs-keyword">\mathcal</span></span><span>{C}</span><span><span class="hljs-built_in">_</span></span><span>{</span><span><span class="hljs-keyword">\sigma</span></span><span><span class="hljs-built_in">_</span></span><span>2}(</span><span><span class="hljs-keyword">\mathcal</span></span><span>{F})
